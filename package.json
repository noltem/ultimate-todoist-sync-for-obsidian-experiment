	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@doist/todoist-api-typescript": "^4.0.2",
		"monkey-around": "^3.0.0"
