{
	"name": "simple-todoist-sync",
	"version": "1.0.2",
	"description": "Sync tasks with Todoist.",
	"main": "main.js",
	"scripts": {
		"dev": "node npm_scripts/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node npm_scripts/esbuild.config.mjs production",
